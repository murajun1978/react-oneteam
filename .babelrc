{
  "presets": [["es2015", { "modules": false }], "es2016", "es2017", "react"],
  "plugins": ["transform-object-rest-spread", "transform-class-properties"],
  "env": {
    "test": {
      "plugins": ["transform-object-rest-spread", "transform-class-properties"],
      "presets": ["es2015"]
    },
    "development": {
      "plugins": ["react-hot-loader/babel"]
    },
    "es": {
      "plugins": ["transform-object-rest-spread", "transform-class-properties", "./config/babel-plugin-transform-import-extension-styl-to-css"],
    },
    "commonjs": {
      "presets": ["es2015"],
      "plugins": ["transform-object-rest-spread", "transform-class-properties", "./config/babel-plugin-transform-import-extension-styl-to-css"],
    }
  }
}
